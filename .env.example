# AI Service Configuration
# Copy this file to .env and add your actual API keys

# OpenAI API Key (for AI-powered chart generation)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Use mock AI responses (set to false when using real OpenAI API)
VITE_USE_MOCK_AI=true

# Google Gemini API Key (if using Gemini)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Anthropic Claude API Key (if using Claude)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# API Base URL (optional, for custom endpoints)
VITE_API_BASE_URL=
